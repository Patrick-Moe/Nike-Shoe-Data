        "df['magic_shoe'] = (df['gender'] == 'Men') & (df['star_rating'] >= 4.0) & (df['no_of_reviews'] >= 20)\n",
        "magic_shoes_m = df[df['magic_shoe']]\n",
        "magic_shoes_max = magic_shoes_m.groupby('star_rating').apply(lambda x: x.nlargest(1, 'no_of_reviews')).reset_index(drop=True)\n",
        "magic_shoes_max = magic_shoes_max.sort_values(['star_rating', 'no_of_reviews'], ascending=[False, False])\n",
        "magic_shoes_max = magic_shoes_max.reset_index(drop=True)\n",
        "print(magic_shoes_max)\n",
