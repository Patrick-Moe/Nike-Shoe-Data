      "cell_type": "code",
      "source": [
        "#perform kruskal-wallis test on sale_price and mean_reviews based on star_rating\n",
        "from scipy.stats import kruskal\n",
        "sale_price = mens['sale_price']\n",
        "star_rating = mens['star_rating']\n",
        "mean_reviews = mens['mean_reviews']\n",
        "statistic, p_value = kruskal(sale_price, star_rating, mean_reviews)\n",
        "\n",
        "# Print the results\n",
        "print(\"Kruskal-Wallis Test Results:\")\n",
        "print(\"Statistic:\", statistic)\n",
        "print(\"P-value:\", p_value)\n",
        "\n",
        "\n",
        "print ('For the men, the p value is greater then 0.05, therefor')\n",
        "print('there is not enough evidenceto reject the null hypothesis.')\n",
        "print('Indicating the observed correlations between number of reviews and star rating are not significant')"
      ],
      "metadata": {
